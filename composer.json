{
  "name": "xiabin/oauth2-sinaweibo",
  "description": "Sina Weibo OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
  "license": "MIT",
  "authors": [
    {
      "name": "xiabin",
      "email": "xiabin19891230@gmail.com",
      "homepage": "http://www.xiabin.me"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "sina weibo"
  ]
  "minimum-stability": "beta",
  "require": {
    "php": ">=5.5.0",
    "league/oauth2-client": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
